<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                android:background="@drawable/b11"
                android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                android:paddingBottom="@dimen/activity_vertical_margin"
                tools:context=".ui.MainActivity"
                android:touchscreenBlocksFocus="false"
    >


    <Button
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="@drawable/oval_try"
        android:text="Regular"
        android:id="@+id/regularButton"
        android:layout_marginTop="30dp"
        android:alpha="1"
        android:textColor="#193655"
        android:singleLine="false"
        android:visibility="visible"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="24sp"
        android:allowUndo="false" />
    <Button
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="@drawable/oval_try"
        android:text="Home"
        android:id="@+id/homeButton"

        android:layout_marginTop="80dp"
        android:alpha="1"
        android:textColor="#193655"
        android:textSize="24sp"
        android:layout_below="@+id/regularButton"
        android:layout_alignLeft="@+id/regularButton"
        android:layout_alignStart="@+id/regularButton" />


</RelativeLayout>
